{
  "name": "claude-code-clean",
  "version": "1.0.0",
  "description": "Clean, readable implementation of Claude Code CLI extracted from bundled JavaScript",
  "type": "module",
  "main": "src/cli/index.js",
  "bin": {
    "claude": "src/cli/index.js"
  },
  "scripts": {
    "start": "node src/cli/index.js",
    "dev": "node --inspect src/cli/index.js",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:unit": "node --experimental-vm-modules node_modules/.bin/jest tests/unit",
    "test:integration": "node --experimental-vm-modules node_modules/.bin/jest tests/integration",
    "test:e2e": "node --experimental-vm-modules node_modules/.bin/jest tests/e2e",
    "test:coverage": "node --experimental-vm-modules node_modules/.bin/jest --coverage",
    "test:watch": "node --experimental-vm-modules node_modules/.bin/jest --watch",
    "test:ci": "node --experimental-vm-modules node_modules/.bin/jest --ci --coverage --watchAll=false",
    "validate": "npm run test && npm run compare",
    "compare": "node tests/validation/compare-implementations.js"
  },
  "keywords": [
    "claude",
    "ai",
    "cli",
    "anthropic",
    "assistant",
    "mcp"
  ],
  "author": "Extracted from original Claude Code implementation",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/bedrock-sdk": "^0.12.0",
    "@anthropic-ai/vertex-sdk": "^0.6.0", 
    "@anthropic-ai/sdk": "^0.30.0",
    "commander": "^11.0.0",
    "chalk": "^5.3.0",
    "ink": "^4.4.0",
    "react": "^18.2.0",
    "open": "^10.0.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.5",
    "execa": "^9.6.0",
    "jest": "^30.0.5",
    "jest-diff": "^30.0.5",
    "nodemon": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "directories": {
    "doc": "docs",
    "lib": "src"
  },
  "repository": {
    "type": "git",
    "url": "extracted from claude-code bundle"
  }
}
